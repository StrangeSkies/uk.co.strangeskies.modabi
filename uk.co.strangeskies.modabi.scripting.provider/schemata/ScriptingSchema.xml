<?xml version="1.0" ?>
<schema xmlns="http://modabi.strangeskies.co.uk/2014-01-01/"
	name="ScriptingSchema">
	<dependencies />

	<types />

	<models>
		<model name="script">
			<property name="objectType" type="type" />

			<property name="objectName" type="string" optional="true" />

			<choice name="scriptType">
				<property name="source" type="url" />

				<sequence>
					<property name="language" type="string" />
					<content name="script" type="string" />
				</sequence>
			</choice>
		</model>
	</models>
</schema>
