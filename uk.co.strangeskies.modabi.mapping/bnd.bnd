	Mapping from objects of a certain model to a different model view.\
\
	${modabi-description}

-runee: JavaSE-1.8

-buildpath: \
	${ss;utilities},\
	${ss;reflection},\
	${mdb;core.api}

Export-Package: uk.co.strangeskies.modabi.mapping

-dependson: \
	uk.co.strangeskies.modabi.bnd

-pluginpath: \
	${pluginpath.modabi;xml}

-plugin: \
	uk.co.strangeskies.modabi.bnd.xml.ModabiXmlBndPlugin

-includeresource: \
	META-INF/schemata=schemata, \
	res
