<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://schema.modabi.strangeskies.co.uk/2014-01-01/"
	name="BenchmarkSchema">
	<dependencies>
		<dependency>BaseSchema</dependency>
	</dependencies>

	<models>
		<model name="person" dataClass="uk.co.strangeskies.modabi.benchmark.PersonType"
			bindingStrategy="CONSTRUCTOR">
			<inputSequence name="constructor" />
			<property name="active" type="boolean" />
			<simpleElement name="firstName" type="string" />
			<simpleElement name="lastName" type="string" />
			<simpleElement name="address1" type="string" />
			<simpleElement name="address2" type="string" />
			<simpleElement name="postCode" type="string" />
			<simpleElement name="city" type="string" />
			<simpleElement name="country" type="string" />
		</model>

		<model name="persons" dataClass="uk.co.strangeskies.modabi.benchmark.PersonsType"
			bindingStrategy="CONSTRUCTOR">
			<inline dataClass="java.util.Set">
				<element baseModel="person" occurrences="0.." inMethod="add"
					outMethodIterable="true" />
			</inline>
		</model>
	</models>
</schema>
