<?xml version="1.0" ?>
<schema xmlns="http://modabi.strangeskies.co.uk/2014-01-01/"
	name="BenchmarkSchema">
	<dependencies>
		<dependency>BaseSchema</dependency>
	</dependencies>

	<types />

	<models>
		<model name="node2"
			dataType="uk.co.strangeskies.modabi.schema.SchemaNode&lt;?, ?&gt;"
			unbindingStrategy="SIMPLE" abstract="true">
			<inputSequence name="configure"
				postInputType="uk.co.strangeskies.modabi.schema.SchemaNodeConfigurator&lt;?, ?&gt;"
				abstract="true" />
			<property name="name" type="qualifiedName" optional="true"
				inMethod="name" />
			<property name="abstract" type="boolean" optional="true"
				inMethod="isAbstract" />
		</model>

		<model name="person" dataClass="uk.co.strangeskies.modabi.benchmark.PersonType"
			bindingStrategy="CONSTRUCTOR">
			<inputSequence name="constructor" />
			<property name="active" type="boolean" />
			<simple name="firstName" type="string" />
			<simple name="lastName" type="string" />
			<simple name="address1" type="string" />
			<simple name="address2" type="string" />
			<simple name="postCode" type="string" />
			<simple name="city" type="string" />
			<simple name="country" type="string" />
		</model>

		<model name="persons" dataClass="uk.co.strangeskies.modabi.benchmark.PersonsType"
			bindingStrategy="CONSTRUCTOR">
			<inline name="personList" outMethod="getPerson" dataClass="java.util.List">
				<complex baseModel="person" outMethod="this" occurrences="0.."
					inMethod="add" outMethodIterable="true" />
			</inline>
		</model>
	</models>
</schema>
